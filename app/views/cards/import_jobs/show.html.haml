- model_class = ImportJob
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:version_id) + ':'
  %br
  = @import_job.version_id
%p
  %strong= model_class.human_attribute_name(:status) + ':'
  %br
  = @import_job.status


= link_to t('.back', :default => t("helpers.links.back")), cards_import_jobs_path, :class => 'btn btn-default'
= link_to t('.edit', :default => t("helpers.links.edit")), edit_cards_import_job_path(@import_job), :class => 'btn btn-default'
= link_to t('.destroy', :default => t("helpers.links.destroy")), cards_import_job_path(@import_job), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
